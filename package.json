{
  "name": "heroku-cli-command",
  "description": "base Heroku CLI command",
  "version": "1.0.3",
  "author": "Jeff Dickey @dickeyxxx",
  "bugs": "https://github.com/heroku/heroku-cli-command/issues",
  "dependencies": {
    "ansi-escapes": "1.4.0",
    "babel-polyfill": "6.23.0",
    "cardinal": "1.0.0",
    "chalk": "1.1.3",
    "got": "6.7.1",
    "heroku-client": "3.0.1",
    "http-call": "1.0.10",
    "lodash.maxby": "4.6.0",
    "netrc": "0.1.4",
    "string": "3.3.3",
    "supports-color": "3.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "codecov": "1.0.1",
    "flow-bin": "0.39.0",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.0.0",
    "jest": "18.1.0",
    "nock": "9.0.6",
    "np": "2.12.0",
    "rimraf": "2.6.0",
    "standard": "8.6.0"
  },
  "homepage": "https://github.com/heroku/heroku-cli-command",
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "repository": "https://github.com/heroku/heroku-command",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js' --source-maps inline",
    "clean": "rimraf lib",
    "copy-flow": "flow-copy-source -v -i '*.test.js' src lib",
    "prepare": "npm run clean && npm run build && npm run copy-flow",
    "release": "np",
    "test": "jest && flow && standard",
    "version": "npm run doc && git add README.md",
    "watch": "babel --watch src -d lib --ignore '*.test.js'"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/linewrap.js",
      "flow-typed"
    ]
  }
}

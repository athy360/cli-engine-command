{
  "name": "cli-engine-command",
  "description": "base CLI command for cli-engine",
  "version": "2.0.0",
  "author": "Jeff Dickey @dickeyxxx",
  "bugs": "https://github.com/heroku/cli-engine-command/issues",
  "dependencies": {
    "ansi-escapes": "1.4.0",
    "babel-runtime": "6.23.0",
    "cardinal": "1.0.0",
    "chalk": "1.1.3",
    "fs-extra": "2.0.0",
    "got": "6.7.1",
    "heroku-client": "3.0.1",
    "http-call": "1.0.11",
    "jest-validate": "19.0.0",
    "lodash.maxby": "4.6.0",
    "netrc": "0.1.4",
    "pretty-format": "19.0.0",
    "string": "3.3.3",
    "supports-color": "3.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-stage-2": "6.22.0",
    "codecov": "1.0.1",
    "flow-bin": "0.39.0",
    "flow-copy-source": "1.1.0",
    "flow-typed": "2.0.0",
    "jest": "18.1.0",
    "jest-junit": "1.2.0",
    "nock": "9.0.6",
    "nodemon": "1.11.0",
    "np": "2.12.0",
    "rimraf": "2.6.0",
    "standard": "8.6.0"
  },
  "homepage": "https://github.com/heroku/cli-engine-command",
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./src",
    "setupFiles": [
      "../test/init.js"
    ]
  },
  "keywords": [
    "heroku",
    "heroku-cli-plugin"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "repository": "heroku/cli-engine-command",
  "scripts": {
    "build": "babel src -d lib --ignore '*.test.js'",
    "clean": "rimraf lib",
    "copy-flow": "flow-copy-source -v -i '*.test.js' src lib",
    "prepare": "npm run clean && npm run build && npm run copy-flow",
    "release": "np",
    "test": "jest && flow && standard && npm run prepare",
    "watch": "nodemon -e js -i lib --exec \"npm run prepare\""
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib",
      "src/output/linewrap.js",
      "flow-typed"
    ]
  }
}
